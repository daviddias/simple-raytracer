{
  "name": "simple-raytracer",
  "version": "1.0.1",
  "description": "standalone version of the JS raytracer built for [distracer.io](http://distracer.io), which was inspired by [jsRayTracer](https://github.com/vjeux/jsRayTracer).",
  "main": "src/index.js",
  "scripts": {
    "lint": "standard",
    "test": "node ./node_modules/.bin/lab -r tap tests/*-test.js | ./node_modules/.bin/tap-spec",
    "test-cov": "node ./node_modules/.bin/lab -t 80 tests/*-test.js",
    "test-cov-html": "node ./node_modules/.bin/lab -r html -o coverage.html tests/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daviddias/simple-raytracer"
  },
  "keywords": [
    "raytracer",
    "simple",
    "javascript",
    "ray",
    "tracing",
    "image",
    "3d",
    "render"
  ],
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "author": "David Dias <mail@daviddias.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daviddias/simple-raytracer/issues"
  },
  "homepage": "https://github.com/daviddias/simple-raytracer",
  "devDependencies": {
    "brfs": "^2.0.2",
    "code": "^5.2.4",
    "domready": "^1.0.8",
    "hapi": "^18.1.0",
    "lab": "^18.0.2",
    "moonboots_hapi": "^7.0.0",
    "png": "^3.0.3",
    "tap-spec": "^5.0.0"
  },
  "dependencies": {
  }
}
